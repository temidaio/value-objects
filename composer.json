{
    "name": "olsza/laravel-value-objects",
    "description": "Useful values objects for Laravel",
    "keywords": [
        "olsza",
        "php",
        "value object",
        "value objects",
        "values object",
        "values objects",
        "laravel"
    ],
    "type": "project",
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/support": "^9.7",
        "michael-rubel/laravel-formatters": "^4.0.1|^5.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10|^6.0",
        "nunomaduro/larastan": "^1.0",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0",
        "mockery/mockery": "^1.4.4",
        "squizlabs/php_codesniffer": "^3.6",
        "pestphp/pest": "^1.16",
        "pestphp/pest-plugin-global-assertions": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-mock": "^1.0",
        "pestphp/pest-plugin-parallel": "^0.2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Olsza",
            "email": "olsza@users.noreply.github.com",
            "homepage": "https://olsza.info",
            "role": "Developer"
        },
        {
            "name": "Michael Rubel",
            "email": "contact@observer.name",
            "homepage": "https://github.com/michael-rubel",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Olsza\\ValueObjects\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olsza\\ValueObjects\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "TaxNumber": "Olsza\\ValueObjects\\Facades\\TaxNumber"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
